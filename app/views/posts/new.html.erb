<% @page_title = "俺飯の投稿" %>

<h6 class="text-center font-weight-bold my-3"><%= @page_title %><h6>
<div class="bg-white container py-4">
<%= form_for @post, html: {:class => "text-center font-weight-bold my-3" } do |f| %>
  <%= render "shared/errors", obj: @post %>
  <%= render 'form_post', f: f %>
<% end %>
</div>
